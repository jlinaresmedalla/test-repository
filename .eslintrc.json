{
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "react/jsx-filename-extension": ["error", { "extensions": [".jsx"] }],
    "react/jsx-props-no-spreading": ["error", { "exceptions": ["Component"] }],
    "react/prop-types": "off"
  }
}
